    "serve-favicon": "^2.5.0",
    "ts-node": "^8.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.0",
    "fest-webpack-loader": "^2.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.3",
