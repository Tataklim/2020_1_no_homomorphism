<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <a href="/" class="l-logo">
        <img src="/static/img/library_music.svg" class="m-medium-icon" alt="logo"/>
    </a>
    <a href="/" class="m-project-name">
        VirusMusic
    </a>
    <input type="text" class="m-search-input" placeholder="Find artist, track or album"/>
    <a class="m-button-without-size">
        <img src="/static/img/search.svg" class="m-medium-icon"/>
    </a>
    <a href="/profile/tracks" class="l-navbar-info is-hidden" id="profile-link">
        <img class="m-navbar-avatar m-medium-icon col" src="/static/img/avatar/default.png" alt="avatar"/>

        <div class="m-navbar-name col"></div>
    </a>
    <a href="/settings" class="l-settings-icon is-hidden">
        <img src="/static/img/settings.svg" class="m-small-icon"/>
    </a>
    <a href="/" class="m-login-button is-hidden" id="logout-link">
        log out
    </a>
    <a href="/signup" class="m-signup-button is-hidden" id="signup-link">
        sign up
    </a>
    <a href="/login" class="m-login-button is-hidden" id="login-link">
        log in
    </a>
    <div class="l-top-search">
    </div>
</fest:template>
