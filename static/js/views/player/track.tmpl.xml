<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">

    <fest:for iterate="data" index="i" value="track">
        <div class="row m-player-track">
            <div class="col">
                <div class="track-photo">
                    <img src="{track.link}" height="50px" width="50px" alt="..."/>
                </div>
            </div>
            <div class="col track-group-names">
                <div class="track-name">
                    <fest:value>track.name</fest:value>
                </div>
                <a class="group-name" href="/player">
                    <fest:value>track.artist</fest:value>
                </a>
            </div>
