<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <div class="trigger-button">
        <img src="/static/img/icons/arrow_forward_ios.svg" class="player-trigger-arrow"
            alt="." height="28"/>
    </div>
    <div class="row">
        <div class="player-trigger-row">
            <img src="/static/img/icons/arrow_forward_ios.svg" class="player-trigger-arrow-row"
                alt="." height="28"/>
        </div>
        <div class="col player-trigger" style="height: 1000px;">
        </div>
        <div class="col main-col">
            <div class="row patch"></div>
            <div class="row l-music-bar">
            </div>
            <div class="container-audio border-bottom">
            </div>
        </div>
        <div class="col side-col">
            <div class="track-list">
                <div class="current-marker" style="top: 5px; height: 50px;"></div>
            </div>
        </div>
    </div>
</fest:template>
