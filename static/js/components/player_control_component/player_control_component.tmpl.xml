<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <div class="timeline row">
        <div class="timeline-back"></div>
        <div class="timeline-front" style="width: 0;"></div>
        <div class="current-time col" style="font-size: 0px;">0:00</div>
        <div class="duration col" style="font-size: 0px;">0:00</div>
    </div>
    <div class="control-elements row">
        <img src="/static/img/previous.svg" class="player-control-button prev" id="prev" alt="prev"/>
        <img src="/static/img/play.svg" class="player-control-button play-pause" alt="play/pause"/>
        <img src="/static/img/next.svg" class="player-control-button next" id="next" alt="next"/>
        <img src="/static/img/shuffle.svg" class="playlist-control-button shuffle" alt="shuffle"/>
        <img src="/static/img/repeat.svg" class="playlist-control-button repeat" alt="repeat"/>
        <img src="/static/img/volume_up.svg" class="volume" alt="volume"/>
        <div class="volume-scale" style="transition-property: opacity, visibility, top;">
            <div class="volume-scale-back"></div>
            <div class="volume-scale-front"></div>
        </div>
    </div>
    <audio>
        <source src=""/>
        Your browser does not Support the audio Tag
    </audio>
</fest:template>
